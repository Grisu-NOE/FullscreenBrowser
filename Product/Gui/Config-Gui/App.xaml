<Application x:Class="At.FF.Krems.Config_Gui.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:toolkit="http://schemas.xceed.com/wpf/xaml/toolkit"
             xmlns:prop="clr-namespace:At.FF.Krems.Config_Gui.Properties"
             StartupUri="MainWindow.xaml">
    <Application.Resources>
        <Style TargetType="{x:Type toolkit:MessageBox}" x:Key="MessageBoxStyle">
            <Setter Property="YesButtonContent" Value="{x:Static prop:Resources.Yes_DE_AT}"/>
            <Setter Property="NoButtonContent" Value="{x:Static prop:Resources.No_DE_AT}"/>
            <Setter Property="CancelButtonContent" Value="{x:Static prop:Resources.Cancel_DE_AT}"/>
        </Style>
    </Application.Resources>
</Application>